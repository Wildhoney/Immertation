{
  "name": "immertation",
  "version": "0.1.18",
  "packageManager": "pnpm@10.24.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/immertation.cjs",
  "module": "./dist/immertation.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/immertation.mjs",
      "require": "./dist/immertation.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:example": "vite build --config vite.config.example.ts",
    "preview": "vite preview",
    "unit": "vitest run src",
    "integration": "playwright test",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "docs": "typedoc --out example/public/docs --entryPoints src/index.ts src/types.ts --excludePrivate --name Immertation --readme none"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@faker-js/faker": "^10.1.0",
    "@mobily/ts-belt": "^3.13.1",
    "@playwright/test": "^1.56.1",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/traverse": "^0.6.37",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.14",
    "commit-and-tag-version": "^12.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "immer": "^10.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "lucide-react": "^0.554.0",
    "@ant-design/icons": "^6.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "react-dom": "^19.2.0",
    "traverse": "^0.6.11",
    "typedoc": "^0.28.14",
    "typescript": "~5.9.3",
    "antd": "^5.29.1",
    "vite": "^7.2.1",
    "vitest": "^4.0.14",
    "wait-on": "^9.0.3"
  },
  "peerDependencies": {
    "@mobily/ts-belt": "^3.0.0",
    "immer": "^6.0.0",
    "lodash": "^4.17.0"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  }
}
