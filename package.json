{
  "name": "immeration",
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "main": "./dist/immeration.cjs",
  "module": "./dist/immeration.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/immeration.mjs",
      "require": "./dist/immeration.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "unit": "vitest run",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,json}'"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@mobily/ts-belt": "^3.13.1",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/traverse": "^0.6.37",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "commit-and-tag-version": "^12.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "immer": "^10.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "traverse": "^0.6.11",
    "typescript": "~5.9.3",
    "vite": "^7.2.1",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "@mobily/ts-belt": "^3.0.0",
    "immer": "^6.0.0",
    "lodash": "^4.17.0"
  }
}
